CREATE TABLE workout(id INTEGER NOT NULL, name VARCHAR NOT NULL, PRIMARY KEY(id));
CREATE TABLE exercise(id INTEGER NOT NULL, workout_id INTEGER NOT NULL REFERENCES workout(id) ON DELETE CASCADE ON UPDATE CASCADE, name VARCHAR NOT NULL, PRIMARY KEY(id));
CREATE TABLE sets(id INTEGER NOT NULL, exercise_id INTEGER NOT NULL REFERENCES exercise(id) ON DELETE CASCADE ON UPDATE CASCADE, weight DOUBLE NOT NULL, reps INTEGER NOT NULL, PRIMARY KEY(id));
CREATE TABLE finished_workout(id INTEGER NOT NULL, workout_id INTEGER, name VARCHAR NOT NULL, created INTEGER, PRIMARY KEY(id));
CREATE TABLE finished_exercise(id INTEGER NOT NULL, finished_workout_id INTEGER NOT NULL REFERENCES finished_workout(id) ON DELETE CASCADE ON UPDATE CASCADE, name VARCHAR NOT NULL, PRIMARY KEY(id));
CREATE TABLE finished_sets(id INTEGER NOT NULL, finished_exercise_id INTEGER NOT NULL REFERENCES finished_exercise(id) ON DELETE CASCADE ON UPDATE CASCADE, weight DOUBLE NOT NULL, reps INTEGER NOT NULL, max_reps INTEGER NOT NULL, PRIMARY KEY(id));