<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="textColor" format="color"/>
    <attr name="currentValue" format="integer"/>
    <attr name="maxValue" format="integer"/>
    <attr name="textSize" format="dimension"/>
    <declare-styleable name="ProgressBar">
        <attr name="textSize"/>
        <attr name="textColor"/>
        <attr name="currentValue"/>
        <attr name="maxValue"/>
        <attr name="barColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="EnterValueBar">
        <attr name="textColor"/>
        <attr name="value" format="string"/>
        <attr name="valueTextSize" format="dimension"/>
        <attr name="label" format="string"/>
        <attr name="labelTextSize" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="VerticalProgressBar">
        <attr name="imageLevel" format="integer"/>
        <attr name="progressBar" format="reference" />
    </declare-styleable>
</resources>