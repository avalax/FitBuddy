CREATE TABLE workout(id INTEGER NOT NULL, name VARCHAR NOT NULL, PRIMARY KEY(id));
CREATE TABLE exercise(id INTEGER NOT NULL, workout_id INTEGER NOT NULL REFERENCES workout(id) ON DELETE CASCADE ON UPDATE CASCADE, name VARCHAR NOT NULL, position INT NOT NULL, PRIMARY KEY(id));
CREATE TABLE sets(id INTEGER NOT NULL, exercise_id INTEGER NOT NULL REFERENCES exercise(id) ON DELETE CASCADE ON UPDATE CASCADE, weight DOUBLE NOT NULL, reps INTEGER NOT NULL, PRIMARY KEY(id));
CREATE TABLE finished_workout(id INTEGER NOT NULL, workout_id INTEGER, name VARCHAR NOT NULL, created DATETIME DEFAULT CURRENT_TIMESTAMP, PRIMARY KEY(id));
CREATE TABLE finished_exercise(id INTEGER NOT NULL, finished_workout_id NOT NULL REFERENCES finished_workout(id) ON DELETE CASCADE ON UPDATE CASCADE, name VARCHAR NOT NULL, weight DOUBLE NOT NULL, reps INTEGER NOT NULL, PRIMARY KEY(id));